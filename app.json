{
  "expo": {
    "name": "4TODO",
    "slug": "4todo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "dotenv cross-var foobar %REACT_APP_GOOGLE_API_KEY%"
      },
      "buildNumber": "1.0.0"
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "config": {
        "googleMaps": {
          "apiKey": "dotenv cross-var foobar %REACT_APP_GOOGLE_API_KEY%"
        }
      },
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "오픈 SW 플랫폼 프로젝트 -- To-do app 개발하기",
    "githubUrl": "https://github.com/OSP04/osp-todo-app"
  }
}
